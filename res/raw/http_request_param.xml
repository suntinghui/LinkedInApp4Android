<?xml version="1.0" encoding="UTF-8"?>

<!-- requestId的值务必与 HttpRequestType 中的相关值保持一致 -->

<root>
    
    <!-- 登录 -->
    <request>
        <requestId>LOGIN</requestId>
        <url>/login</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 登出 -->
    <request>
        <requestId>LOGOUT</requestId>
        <url>/logout</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 修改密码 -->
    <request>
        <requestId>UPDATEPWD</requestId>
        <url>/account/password/update</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 注册 -->
    <request>
        <requestId>REGISTER</requestId>
        <url>/account/create</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 查看个人基本信息 -->
    <request>
        <requestId>PROFILE_BASIC</requestId>
        <url>/profile/${id}/basic</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 查看全部个人信息 -->
    <request>
        <requestId>PROFILE_ALL</requestId>
        <url>/profile/${id}/all</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 提交个人信息－后台匹配 -->
    <request>
        <requestId>PROFILE_MATCH</requestId>
        <url>/profile/me/match</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 更新个人信息 -->
    <request>
        <requestId>PROFILE_UPDATE</requestId>
        <url>/profile/me/update</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 查看时间轴 -->
    <request>
        <requestId>TIMELINE_LIST</requestId>
        <url>/timeline/${id}/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 添加时间轴结点 -->
    <request>
        <requestId>TIMELINE_NODE_CREATE</requestId>
        <url>/timeline/me/node/create</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 更新时间轴结点 -->
    <request>
        <requestId>TIMELINE_NODE_UPDATE</requestId>
        <url>/timeline/me/node/update</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 删除时间轴结点 -->
    <request>
        <requestId>TIMELINE_NODE_DELETE</requestId>
        <url>/timeline/me/node/delete</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 根据选中履历推荐好友 -->
    <request>
        <requestId>TIMELINE_NODE_NEWFRIENDS_LIST</requestId>
        <url>/timeline/me/node/${nodeID}/newfriends/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 我关注的好友列表 -->
    <request>
        <requestId>FRIENDS_LIST</requestId>
        <url>/friends/me/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 关注我的人列表 -->
    <request>
        <requestId>FRIENDS_FUNS_LIST</requestId>
        <url>/friends/me/funs/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 关注某人（添加好友） -->
    <request>
        <requestId>FRIENDS_FOLLOW</requestId>
        <url>/friends/${id}/follow</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 取消关注某人（删除好友） -->
    <request>
        <requestId>FRIENDS_UNFOLLOW</requestId>
        <url>/friends/${id}/unfollow</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 获取母校介绍 -->
    <request>
        <requestId>COLLEGE_INTRODUCT</requestId>
        <url>/college/info</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 获取公告标题列表 -->
    <request>
        <requestId>COLLEGE_BROADCAST_LIST</requestId>
        <url>/college/broadcast/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 获取公告详情 -->
    <request>
        <requestId>COLLEGE_BROADCAST_DETAIL</requestId>
        <url>/college/broadcast/${id}/detail</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 获取活动类型列表 -->
    <request>
        <requestId>COLLEGE_EVENT_TYPE_LIST</requestId>
        <url>/college/event/type/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 获取活动列表 -->
    <request>
        <requestId>COLLEGE_EVENT_LIST</requestId>
        <url>/college/event/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 获取活动详情 -->
    <request>
        <requestId>COLLEGE_EVENT_DETAIL</requestId>
        <url>/college/event/${id}/detail</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 获取活动成员列表 -->
    <request>
        <requestId>COLLEGE_EVENT_PARTICIPANT_LIST</requestId>
        <url>/college/event/${id}/participant/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 关注活动 -->
    <request>
        <requestId>COLLEGE_EVENT_FOLLOW</requestId>
        <url>/college/event/${id}/follow</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 取消关注活动 -->
    <request>
        <requestId>COLLEGE_EVENT_UNFOLLOW</requestId>
        <url>/college/event/${id}/unfollow</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 参加活动 -->
    <request>
        <requestId>COLLEGE_EVENT_JOIN</requestId>
        <url>/college/event/${id}/join</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 取消参加活动 -->
    <request>
        <requestId>COLLEGE_EVENT_QUIT</requestId>
        <url>/college/event/${id}/quit</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 回馈母校记录列表 -->
    <request>
        <requestId>COLLEGE_FEEDBACK_LIST</requestId>
        <url>/college/feedback/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 记录回馈母校行为 -->
    <request>
        <requestId>COLLEGE_FEEDBACK_CREATE</requestId>
        <url>/college/feedback/me/create</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 申请校友龙卡 -->
    <request>
        <requestId>COLLEGE_CARD_APPLY</requestId>
        <url>/college/card/apply</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 查看校友龙卡申请进度 -->
    <request>
        <requestId>COLLEGE_ALUCARD_STATUS</requestId>
        <url>/college/alucard/status</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 申请捐赠 -->
    <request>
        <requestId>COLLEGE_FEEDBACK_APPLY</requestId>
        <url>/college/feedback/apply</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 查看系统推荐的圈子列表 -->
    <request>
        <requestId>GROUP_LIST</requestId>
        <url>/group/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 查看我加入的圈子列表 -->
    <request>
        <requestId>GROUP_ME_LIST</requestId>
        <url>/group/me/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 查看我创建的圈子列表 -->
    <request>
        <requestId>GROUP_MY_LIST</requestId>
        <url>/group/my/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 查看圈子成员 -->
    <request>
        <requestId>PARTICIPANT_LIST</requestId>
        <url>/group/${id}/participant/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 加入圈子 -->
    <request>
        <requestId>GROUP_JOIN</requestId>
        <url>/group/${id}/join</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 删除圈子 -->
    <request>
        <requestId>GROUP_DELETE</requestId>
        <url>/group/${id}/delete</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 退出圈子 -->
    <request>
        <requestId>GROUP_QUIT</requestId>
        <url>/group/${id}/quit</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 获取圈子发言列表 -->
    <request>
        <requestId>GROUP_COMMENT_LIT</requestId>
        <url>/group/${id}/comment/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 圈子中发言 -->
    <request>
        <requestId>GROUP_COMMENT_CREATE</requestId>
        <url>/group/${id}/comment/create</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 创建圈子 -->
    <request>
        <requestId>GROUP_CREATE</requestId>
        <url>/group/create</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 获取圈子发言列表圈子 -->
    <request>
        <requestId>GROUP_COMMENT_LIST</requestId>
        <url>/group/${id}/comment/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 获取圈子发言列表圈子 -->
    <request>
        <requestId>PUBLISH_COMMENT</requestId>
        <url>/group/${id}/comment/create</url>
        <method>POST</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 找同学 -->
    <request>
        <requestId>FRIENDS_SEARCH</requestId>
        <url>/friends/search</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 获取置顶媒体信息列表 -->
    <request>
        <requestId>MEDIA_TOPLIST</requestId>
        <url>/media/top/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 获取普通媒体信息列表 -->
    <request>
        <requestId>MEDIA_LIST</requestId>
        <url>/media/list</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
    <!-- 根据ID获取媒体信息详情 -->
    <request>
        <requestId>MEDIA_DETAIL</requestId>
        <url>/media/${id}/detail</url>
        <method>GET</method>
        <contentType>application/json</contentType>
    </request>
    
</root>
